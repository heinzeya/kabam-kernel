{
  "name": "mwc_core",
  "version": "0.0.2",
  "description": "MyWebClass core node.js application to be extended by loaded automatically middleware plugins",
  "main": "index.js",
  "engines": {
    "node": "0.10.x",
    "npm": "1.3.x"
  },
  "scripts": {
    "start": "node example/example.js",
    "test": "./node_modules/.bin/mocha --reporter spec",
    "blanket": { "pattern": ["index.js", "models/**/*.js", "public/**/*.js", "routes/**/*.js"] }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mywebclass/mwc_core.git"
  },
  "dependencies": {
    "async": "*",
    "express": "*",
    "redis": "*",
    "connect-redis": "*",
    "connect-flash": "*",
    "passport": "*",
    "passport-local": "*",
    "passport-hash": "*",
    "passport-google": "*",
    "passport-github": "*",
    "passport-twitter":"*",
    "passport-facebook":"*",
    "mongoose": "*",
    "hat": "*",
    "moment": "*",
    "toobusy": "~0.2.3"
  },
  "devDependencies": {
    "grunt": "~0.4.1",
    "grunt-cli": "*",
    "grunt-contrib-watch": "~0.5.0",
    "grunt-contrib-jshint": "~0.6.0",
    "should": "~1.2.2",
    "mocha": "~1.12.0",
    "grunt-mocha-cov": "0.0.7",
    "blanket": "~1.1.5"
  },
  "optionalDependencies": {
    "hiredis": "*"
  },
  "keywords": [
    "mywebclass",
    "core"
  ],
  "author": "Anatolij Ostroumov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mywebclass/mwc_core/issues"
  }
}
